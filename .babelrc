{
  "presets": [
    "react",
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [ "last 2 versions", "ie >= 11"]
        }
      }
    ],
    "stage-2"
  ],
  "env": {
    "test": {
      "presets": [["env"], "react", "stage-2"]
    }
  },
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties"
  ]
}
